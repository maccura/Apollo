/* base_push_button类 Regular*/
QWidget
{
	/* font-family: 思源黑体 CN Regular;*/
	/* font: 75 14pt "思源黑体 CN Bold"; */
	/* font: 57 14pt "思源黑体 CN Medium"; */
	font: 57 14pt "HarmonyOS Sans SC Medium";
}

QLabel
{
	font-size: 14px;
	color: #000000;
}

QToolTip 
{
	background-color: #262626; 
	opacity: 0.8;
	border: 1px solid black;
	color: #ffffff;	
}

QComboBox
{
	background-color: #f2f2f2; 
	color: #3c3c3c; 
	border-radius: 4px;
	font-size: 14px;
	padding-left: 10px;
}

QScrollBar 
{
    background-color: #F0F0F0;
    border: none;
    width: 10px;
}


QLineEdit
{
	background-color: #f2f2f2;
	border-radius: 4px;
}

QSizeGrip
{
	background-color: transparent; 
}

QSpinBox::up-button 
{
	image: url(:/spinbox_up);
}

QSpinBox::down-button 
{
	image: url(:/spinbox_down);
}

QSpinBox::down-button:pressed 
{
	position: relative; 
	top: 2px;
}

QSpinBox::up-button:pressed 
{
	position: relative; bottom: 1px;
}

QComboBox::drop-down 
{
	border-radius: 4px; 
	color: #ffffff; 
}

QComboBox::down-arrow 
{
	image: url(:/arr_down);
	subcontrol-origin: padding;
    subcontrol-position: left;
    right: 5px;
}

QComboBox::down-arrow:pressed 
{
	position: relative; 
	top: 2px; 
}


QScrollBar::handle 
{
	background-color: #CCCCCC;
	border-radius: 5px;
}
QScrollBar::add-line, QScrollBar::sub-line 
{
	background-color: #F0F0F0;
}

QScrollBar::add-page, QScrollBar::sub-page 
{
	background-color: none;
}

ReportDesign--base_mask_dialog
{
	background-color: transparent;
}

ReportDesign--base_mask_dialog QToolBar,
ReportDesign--base_mask_dialog QMenuBar
{
	border-radius: 6px;
	font: 12pt "HarmonyOS Sans SC Medium";
}

ReportDesign--gbase_push_button
{
	font-size: 16px;
	border-radius: 6px; 
	background-color: #57bd7c; 
	border-width: 1px; 
	color: #fefefe;
} 

ReportDesign--base_push_button
{
	font-size: 16px;
	border-radius: 6px; 
	background-color: #ffffff; 
	border-width: 1px; 
	border-style: solid; 
	border-color: #e4e7ec;
}

ReportDesign--base_push_button#previewReportButton
{
	border-color: #57bd7c; 
	color: #57bd7c;  
	font-size: 16px;
	border-width: 1px; 
	border-radius: 4px;
	padding: 2px 5px 2px 5px;
}

ReportDesign--gbase_push_button#exportReportButton
{
	color: #ffffff; 
	font-size: 16px; 
	border-radius: 4px;
	padding: 2px 5px 2px 5px;
} 

ReportDesign--design_main_window .QMenu
{ 
	background-color: white; 
	border-radius: 6px;
}

ReportDesign--design_main_window .QMenu::indicator
{ 
	image: none;
}

ReportDesign--design_main_window .QMenu::item 
{
	font-size: 14px; 
	background-color: #ffffff; 
	color: #333333;
	padding: 20px 24px;
	image: none;
} 

ReportDesign--design_main_window .QMenu::item:selected 
{
	background-color: #e3f2e8; 
	color: #57bd7c;
}

ReportDesign--design_main_window .QMenu::right-arrow 
{
	margin-right: 10px; 
}

ReportDesign--design_main_window .QToolButton 
{
	border: none;
}

ReportDesign--design_main_window .QToolButton::menu-indicator
{
	image:none;
}

ReportDesign--design_main_window .QToolButton:hover 
{
	border-radius: 50%; 
	background-color: #d9d9d9; 
}

ReportDesign--design_main_window .QDockWidget .QWidget
{
	background-color: white;
}

.ReportDesign--design_main_window QPushButton#watermarkSeting
{
	color: #57bd7c; 
	font-size: 14px; 
	border-radius: 6px; 
	background-color: #ffffff;
}

