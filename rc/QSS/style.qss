{
	color: #565656; 
	font-size: 14px;
}*/

ReportDesign--Item_button
{
	background-color: #f7f7f7; 
	border-radius: 4px;
}

ReportDesign--Item_button .QLabel
{
	color: #3c3c3c; 	
	font-size: 14px;
}

ReportDesign--Item_button .QLabel#mask
{
	background-color: #7f7f7f7f;
}

ReportDesign--dialog_background#backLabel
{
	background-color: rgba(128, 128, 128, 128);
} 

ReportDesign--message_info_dialog QLabel
{
	font-size: 16px;
}

ReportDesign--module_type_dialog QTableWidget
{
	outline: none;
	border: none;
}

ReportDesign--module_type_dialog QTableWidget::item 
{
	padding: 2px;
}

ReportDesign--module_type_dialog QTableWidget::item:selected
{
	background-color: #e3f2e8; 
	border-radius: 4px;
	border-width: 2px; 
	border-style: solid; 
	border-color: #47b347;
}

ReportDesign--module_file_button 
{
	border-radius: 4px;
	border-color: #a8b2c2;
	border-width: 1px; 
	border: 1px solid black;
}

ReportDesign--module_file_button .QLabel
{
	font-size: 14px;
}

ReportDesign--module_file_button#emptyFileButton QLabel
{
	font-size: 16px;
}

ReportDesign--variable_edit_dialog QTextEdit
{
	border-radius: 4px; 
	border-width: 1px; 
	border-style: solid; 
	border-color: #e4e7ec;
	font-size: 14px; 
}

ReportDesign--variable_edit_dialog QTableWidget
{
	outline: none;
}

ReportDesign--variable_edit_dialog QTableWidget::item:selected
{
	background-color: #e3f2e8;
	border-color: #e3f2e8;
	font-size: 14px; 
}

ReportDesign--variable_edit_dialog  QHeaderView
{
	background-color: #ffffff;
	font-size: 14px;
}

ReportDesign--watermark_setting_dialog QTextEdit
{
	background-color: #f2f2f2; 
	border-radius: 4px;
}

ReportDesign--watermark_setting_dialog QPushButton#watermarkImageImport
{
	background-color: #f2f2f2; 
	border-radius: 4px; 
	font-size: 16px; 
	color: #57bd7c;
}

ReportDesign--prop_color_button,
ReportDesign--prop_line_edit QLabel, ReportDesign--prop_line_edit QLineEdit,
ReportDesign--prop_spinbox_widget QSpinBox,ReportDesign--prop_spinbox_widget QLabel,
ReportDesign--prop_combox
{
	background-color: #f2f2f2; 
	border-radius: 2px; 
	font-size: 12px; 
}

ReportDesign--base_prop_widget QLabel
{
	font-size: 13px; 
}

